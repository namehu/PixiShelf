{
  "name": "@artisan-shelf/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsup src/index.ts --dts --format esm,cjs --minify --out-dir dist",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "type-check": "tsc --noEmit",
    "prisma:generate": "prisma generate",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.17.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.2",
    "@fastify/cors": "^9.0.1",
    "fastify-plugin": "^4.5.1",
    "pino": "^9.2.0",
    "mime-types": "^2.1.35",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/mime-types": "^2.1.4",
    "eslint": "^8.51.0",
    "prisma": "^5.17.0",
    "tsup": "^8.1.0",
    "tsx": "^4.7.0",
    "typescript": "^5.2.2"
  }
}